<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTab">
        <attr name="text" format="string" />
        <attr name="imgRes" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="Tournament">
        <attr name="countryName" format="string" />
        <attr name="tournamentName" format="string" />
        <attr name="tournamentImgRes" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="Match">
        <attr name="matchTime" format="string" />
        <attr name="currentTime" format="string" />
        <attr name="teamHome" format="string" />
        <attr name="teamAway" format="string" />
        <attr name="resultHome" format="string" />
        <attr name="resultAway" format="string" />

        <attr name="teamHomeImgRes" format="reference|color" />
        <attr name="teamAwayImgRes" format="reference|color" />
    </declare-styleable>

</resources>